{
    "name": "zoomplanner",
    "version": "1.0",
    "manifest_version": 2,
    "permissions": [
        "*",
        "http://*/",
        "https://*/"
    ],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "browser_action": {
        "default_icon": "zoomplanner.png",
        "default_popup": "popup.html",
        "default_title": "zoomplanner"
    }
}